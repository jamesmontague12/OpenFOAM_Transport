FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rhof;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -3 0 0 0 0 0 ];

internalField   uniform 1000;

boundaryField
{
	mirror
	{
		type symmetryPlane;
	}
	frontAndBack
    {
        type            empty;
    }
    ".*"
    {
        type            zeroGradient;
    }
}


